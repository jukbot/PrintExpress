{
  "name": "nextjs-commerce",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "find:unused": "next-unused"
  },
  "prettier": {
    "singleQuote": true
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "dependencies": {
    "@bigcommerce/storefront-data-hooks": "^1.0.2",
    "@headlessui/react": "^0.2.0",
    "@reach/portal": "^0.11.2",
    "@react-aria/overlays": "^3.5.0",
    "@tailwindcss/ui": "^0.7.2",
    "@types/lodash.throttle": "^4.1.6",
    "@vercel/fetch": "^6.1.0",
    "autoprefixer": "^10.0.2",
    "bowser": "^2.11.0",
    "classnames": "^2.2.6",
    "email-validator": "^2.0.4",
    "js-cookie": "^2.2.1",
    "keen-slider": "^5.3.5",
    "lodash.random": "^3.2.0",
    "lodash.throttle": "^4.1.1",
    "next": "^10.0.3",
    "next-seo": "^4.17.0",
    "next-themes": "^0.0.10",
    "postcss-nesting": "^7.0.1",
    "react": "^17.0.1",
    "react-aria": "^3.1.1",
    "react-dom": "^17.0.1",
    "react-intersection-observer": "^8.31.0",
    "react-merge-refs": "^1.1.0",
    "react-ticker": "^1.2.2",
    "tailwindcss": "^2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.3",
    "@types/classnames": "^2.2.11",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash.random": "^3.2.6",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "next-unused": "^0.0.3",
    "postcss": "^8.1.10",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2"
  },
  "license": "MIT"
}
